{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
    'helpers:pinGitHubActionDigests',
    ':automergePr',
    ':disableRateLimiting',
    ':dependencyDashboard',
    ':semanticCommits',
    ':timezone(Europe/Amsterdam)',
  ],
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/extension\\.toml$/',
      ],
      matchStrings: [
        'repository = "https://github\\.com/acristoffers/tree-sitter-matlab"\\s+rev = "(?<currentDigest>[a-f0-9]{40})"',
      ],
      currentValueTemplate: 'main',
      depNameTemplate: 'tree-sitter-matlab',
      datasourceTemplate: 'git-refs',
      packageNameTemplate: 'https://github.com/acristoffers/tree-sitter-matlab',
    },
  ],
  packageRules: [
    {
      matchDatasources: [
        'git-refs',
      ],
      matchDepNames: [
        'tree-sitter-matlab',
      ],
      semanticCommits: 'enabled',
      semanticCommitType: 'refactor',
      semanticCommitScope: 'deps',
      commitMessageTopic: 'tree-sitter-matlab',
      commitMessageAction: 'update',
      commitMessageExtra: '',
      automerge: false,
      prCreation: 'not-pending',
      labels: [
        'dependencies',
        'tree-sitter-matlab',
      ],
    },
  ],
}
